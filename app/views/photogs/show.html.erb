<% @page_title = 'Photog' %>
<div class="span-22 prepend-1 append-1 last" id="second">
<h2 style="text-align:center;">
  <%= link_to(image_tag(@photog.profile_image_url, 
        :size => "45x45", :alt => @photog.screen_name, :class => 'twitter-avatar'), @photog.twitter_url) %> 
  <%= link_to "@#{@photog.screen_name}", @photog.twitter_url %> 
  has tweeted 
  <%= pluralize(@photog.photos.size, 'daily shoot assignment') %>
</h2>
</div>

<%= render :partial => 'shared/photowall', 
           :locals => {:photos  => @photos, 
                       :caption => 'assignment'} %>

<div>
  <%= will_paginate @photos %>
</div>

<% if admin? -%>
<div>
  <%= link_to 'logout', logout_url %>
</div>
<% end -%>